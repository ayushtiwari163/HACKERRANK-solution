# HACKERRANK-solution
problem solvung
#include<stdio.h>
int main()
{
  int n,a[1000],i,j,t;
  scanf("%d" ,&n);
  for(i=0;i<n;i++)
  {
   scanf("%d" ,&a[i]);
  }
  for(i=0;i<n;i++)
  {
    t=a[i]%10;
  if(a[i]<38)
  printf("%d" ,a[i]);
  else if(t==1)
  printf("%d" ,a[i]);
  else if(t==2)
  printf("%d" ,a[i]);
  else if(t==3)
  printf("%d" ,a[i]+2);
  else if(t==4)
  printf("%d" ,a[i]+1);
  else if(t==5)
  printf("%d" ,a[i]);
  else if(t==0)
  printf("%d" ,a[i]);
  else if(t==6)
  printf("%d" ,a[i]);
  else if(t==7)
  printf("%d" ,a[i]);
  else if(t==8)
  printf("%d" ,a[i]+2);
  else if(t==9)
  printf("%d" ,a[i]+1);
  
 if(i!=n-1)
  printf("\n");
}
return 0;
}
